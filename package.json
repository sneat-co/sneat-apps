		"ts-node": "10.9.2",
		"typescript": "5.9.3",
		"verdaccio": "^6.0.5"
	},
	"lint-staged": {
		"*.{html,ts,js}": "eslint --no-cache --fix",
		"*.{html,ts,js,css,md}": "prettier --write"
	},
	"nx": {
		"includedScripts": []
	},
	"madge": {
		"detectiveOptions": {
			"ts": {
				"skipAsyncImports": true
			}
		}
	}
}
