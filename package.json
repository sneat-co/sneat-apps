{
	"name": "sneat-apps",
	"version": "0.0.0",
	"license": "MIT",
	"packageManager": "pnpm@10.26.0",
	"scripts": {
		"nx": "NODE_OPTIONS=--no-deprecation nx",
		"ng": "nx",
		"postinstall_legacy": "node ./decorate-angular-cli.js && ngcc --properties es2022 browser module main --first-only --create-ivy-entry-points",
		"start": "nx serve",
		"build": "nx build",
		"build:ci": "nx build",
		"test": "nx test",
		"prepare": "git config core.hooksPath .git-hooks || echo 'Not in a git repo'",
		"firebase-emulators:dev": "cross-env firebase emulators:start --only auth,firestore --config ../sneat-go-backend/firebase/firebase.json",
		"firebase-emulators:ci": "cross-env firebase emulators:start --only auth,firestore --config ../../_actions/sneat-co/sneat-go-backend/main/firebase/firebase-ci.json",
		"cy:serve:ci": "cross-env nx serve sneat-app --configuration=e2e-ci --port=4205",
		"cy:test:ci": "cross-env nx e2e-ci sneat-app --browser chrome --baseUrl=http://localhost:4205",
		"cy:open:ci": "cross-env nx e2e-ci sneat-app --watch --browser chrome --baseUrl=http://localhost:4205",
		"cy:test:dev": "cross-env nx e2e sneat-app --configuration=development --headed --browser chrome --baseUrl=http://localhost:4205",
		"cy:test:prod": "cross-env nx e2e sneat-app --configuration=production --browser chrome --baseUrl=http://localhost:4205",
		"cy:open:dev": "cross-env nx e2e sneat-app --configuration=development --watch --browser chrome",
		"cy:open:prod": "cross-env nx e2e sneat-app --configuration=production --watch --browser chrome",
		"pw:pretest": "tsc --incremental -p tests/tsconfig.json",
		"pw:test": "playwright test tests -c tests-out",
		"serve:help": "serve --help"
	},
	"private": true,
	"pnpm": {
		"overrides": {
			"rxfire>tslib": "~2.8.0"
		},
		"onlyBuiltDependencies": [
			"@firebase/util",
			"@parcel/watcher",
			"@swc/core",
			"core-js",
			"cypress",
			"esbuild",
			"lmdb",
			"msgpackr-extract",
			"nx",
			"oxc-resolver",
			"protobufjs",
			"unrs-resolver"
		]
	},
	"dependencies": {
		"@angular/animations": "21.0.5",
		"@angular/common": "21.0.5",
		"@angular/compiler": "21.0.5",
		"@angular/core": "21.0.5",
		"@angular/fire": "20.0.1",
		"@angular/forms": "21.0.5",
		"@angular/platform-browser": "21.0.5",
		"@angular/platform-browser-dynamic": "21.0.5",
		"@angular/router": "21.0.5",
		"@capacitor-firebase/authentication": "7.5.0",
		"@capacitor/android": "8.0.0",
		"@capacitor/core": "8.0.0",
		"@capacitor/ios": "8.0.0",
		"@ctrl/ngx-codemirror": "^7.0.0",
		"@firebase/auth": "1.12.0",
		"@firebase/firestore": "4.9.3",
		"@firebase/firestore-types": "3.0.3",
		"@ionic-native/core": "5.36.0",
		"@ionic-native/splash-screen": "5.36.0",
		"@ionic-native/status-bar": "5.36.0",
		"@ionic/angular": "8.7.13",
		"@ionic/core": "8.7.13",
		"@nx/angular": "22.2.6",
		"@sentry/angular": "10.31.0",
		"@sentry/browser": "10.31.0",
		"@sneat/random": "0.0.4",
		"@sneat/wormhole": "^0.0.6",
		"@types/tabulator-tables": "6.3.0",
		"@types/telegram-web-app": "^9.0.0",
		"angularx-qrcode": "^21.0.0",
		"codemirror": "^6.0.1",
		"date-fns": "4.1.0",
		"firebase": "12.7.0",
		"ionicons": "8.0.13",
		"posthog-js": "1.307.1",
		"rxjs": "7.8.2",
		"tabulator-tables": "6.3.1",
		"ts-md5": "2.0.1",
		"tslib": "2.8.1",
		"zone.js": "0.16.0"
	},
	"devDependencies": {
		"@angular-devkit/architect": "0.2100.3",
		"@angular-devkit/build-angular": "21.0.3",
		"@angular-devkit/core": "21.0.3",
		"@angular-devkit/schematics": "21.0.3",
		"@angular-eslint/eslint-plugin": "21.1.0",
		"@angular-eslint/eslint-plugin-template": "21.1.0",
