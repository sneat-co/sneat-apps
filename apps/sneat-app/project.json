{
	"name": "sneat-app",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"projectType": "application",
	"sourceRoot": "apps/sneat-app/src",
	"prefix": "sneat",
	"targets": {
		"build": {
			"executor": "@angular-devkit/build-angular:browser",
			"outputs": ["{options.outputPath}"],
			"options": {
				"outputPath": "dist/apps/sneat-app",
				"index": "apps/sneat-app/src/index.html",
				"main": "apps/sneat-app/src/main.ts",
				"polyfills": ["zone.js"],
				"allowedCommonJsDependencies": ["qrcode"],
				"tsConfig": "apps/sneat-app/tsconfig.app.json",
				"inlineStyleLanguage": "scss",
				"assets": [
					"apps/sneat-app/src/favicon.ico",
					"apps/sneat-app/src/assets",
					{
						"glob": "**/*.svg",
						"input": "node_modules/ionicons/dist/ionicons/svg",
						"output": "./svg"
					}
				],
				"styles": ["apps/sneat-app/src/styles.scss"],
				"scripts": []
			},
			"configurations": {
				"production": {
					"budgets": [
						{
							"type": "initial",
							"maximumWarning": "2.5mb",
							"maximumError": "3mb"
						},
						{
							"type": "anyComponentStyle",
							"maximumWarning": "2kb",
							"maximumError": "4kb"
						}
					],
					"fileReplacements": [
						{
							"replace": "apps/sneat-app/src/environments/environment.ts",
							"with": "apps/sneat-app/src/environments/environment.prod.ts"
						}
					],
					"outputHashing": "all"
				},
				"development": {
					"buildOptimizer": false,
					"optimization": false,
					"vendorChunk": true,
					"extractLicenses": false,
					"sourceMap": true,
					"namedChunks": true,
					"fileReplacements": [
						{
							"replace": "libs/app/src/environments/environment.ts",
							"with": "libs/app/src/environments/environment.local.ts"
						}
					]
				},
				"e2e-development": {
					"optimization": false,
					"outputHashing": "all",
					"sourceMap": true,
					"namedChunks": true,
					"aot": true,
					"extractLicenses": false,
					"vendorChunk": true,
					"buildOptimizer": true,
					"fileReplacements": [
						{
							"replace": "libs/app/src/environments/environment.ts",
							"with": "libs/app/src/environments/environment.e2e.ts"
						}
					]
				},
				"e2e-production": {
					"optimization": true,
					"outputHashing": "all",
					"sourceMap": false,
					"namedChunks": false,
					"aot": true,
					"extractLicenses": true,
					"vendorChunk": false,
					"buildOptimizer": true,
					"fileReplacements": [
						{
							"replace": "libs/app/src/environments/environment.ts",
							"with": "libs/app/src/environments/environment.e2e.ts"
						}
					]
				},
				"e2e-ci": {
					"optimization": true,
					"outputHashing": "all",
					"sourceMap": false,
					"namedChunks": false,
					"aot": true,
					"extractLicenses": true,
					"vendorChunk": false,
					"buildOptimizer": true,
					"fileReplacements": [
						{
							"replace": "libs/app/src/environments/environment.ts",
							"with": "libs/app/src/environments/environment.ci.ts"
						}
					]
				}
			},
			"defaultConfiguration": "production"
		},
		"serve": {
			"executor": "@angular-devkit/build-angular:dev-server",
			"configurations": {
				"production": {
					"buildTarget": "sneat-app:build:production"
				},
				"development": {
					"buildTarget": "sneat-app:build:development"
				},
				"e2e-development": {
					"buildTarget": "sneat-app:build:e2e-development"
				},
				"e2e-production": {
					"buildTarget": "sneat-app:build:e2e-production"
				},
				"e2e-ci": {
					"buildTarget": "sneat-app:build:e2e-ci"
				}
			},
			"defaultConfiguration": "development",
			"continuous": true
		},
		"extract-i18n": {
			"executor": "@angular-devkit/build-angular:extract-i18n",
			"options": {
				"buildTarget": "sneat-app:build"
			}
		},
		"lint": {
			"executor": "@nx/eslint:lint"
		},
		"test": {
			"executor": "@nx/jest:jest",
			"outputs": ["{workspaceRoot}/coverage/apps/sneat-app"],
			"options": {
				"jestConfig": "apps/sneat-app/jest.config.ts",
				"tsConfig": "apps/sneat-app/tsconfig.spec.json"
			}
		},
		"e2e": {
			"executor": "@nx/cypress:cypress",
			"options": {
				"cypressConfig": "apps/sneat-app/cypress.config.ts",
				"testingType": "e2e",
				"devServerTarget": "sneat-app:serve",
				"port": "cypress-auto"
			},
			"configurations": {
				"development": {
					"devServerTarget": "sneat-app:serve:e2e-development"
				},
				"production": {
					"devServerTarget": "sneat-app:serve:e2e-production"
				}
			}
		},
		"e2e-ci": {
			"executor": "@nx/cypress:cypress",
			"options": {
				"cypressConfig": "apps/sneat-app/cypress.config.ts",
				"testingType": "e2e",
				"skipServe": true
			}
		},
		"e2e-playwright": {
			"executor": "nx:run-commands",
			"options": {
				"command": "npx playwright test",
				"cwd": "."
			},
			"configurations": {
				"development": {
					"command": "npx playwright test --project=chromium"
				},
				"debug": {
					"command": "npx playwright test --debug --project=chromium"
				},
				"ui": {
					"command": "npx playwright test --ui --project=chromium"
				}
			}
		}
	}
}
